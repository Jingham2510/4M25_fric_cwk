<?xml version="1.0" encoding="UTF-8"?>




<!-- Robot definition -->
<robot name = "IRB_6400">

    <!-- Orange Colour Definition-->
    <material name="ABB_Orange">
        <color rgba ="0.96 0.47 0.156 1"/>    
    </material>


    <!-- Intertia macro definition (src: https://github.com/HumaRobotics/mybot_gazebo_tutorial/blob/master/mybot_description/urdf/macros.xacro) -->

    <macro name="cylinder_inertia" params="m r h">
        <origin rpy = "0 0 0" xyz = "0 0 0"/>
        <inertia  ixx="${m*(3*r*r+h*h)/12}" ixy = "0" ixz = "0"
                  iyy="${m*(3*r*r+h*h)/12}" iyz = "0"
                  izz="${m*r*r/2}" /> 
      </macro>

      <macro name="box_inertia" params="m x y z">
        <origin rpy = "0 0 0" xyz = "0 0 0"/>
        <inertia  ixx="${m*(y*y+z*z)/12}" ixy = "0" ixz = "0"
                  iyy="${m*(x*x+z*z)/12}" iyz = "0"
                  izz="${m*(x*x+z*z)/12}" /> 
      </macro>


    <!--Link descriptions -->
    <link name ="base">
        <visual>
            <geometry>
                <cylinder length="0.14" radius="0.76"/>
            </geometry>
            <material name="ABB_Orange"/>
        </visual>

        <collision>
            <geometry>
                <cylinder length="0.14" radius="0.76"/>
            </geometry>
        </collision>

    <inertial>
      <mass value="100"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>

        <static>true</static>

    </link>


    <link name = "link_1">
        <visual>
            <geometry>
                <box size="0.2 0.2 0.8"/>
            </geometry>            
            <material name="ABB_Orange"/>
        </visual>
    
        <collision>
            <geometry>
                <box size="0.2 0.2 0.8"/>
            </geometry>
        </collision>

    <inertial>
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>


    </link>


    <link name = "link_2">
        <visual>
            <geometry>
                <box size="0.2 0.2 1.05"/>
            </geometry>            
            <material name="ABB_Orange"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.2 0.2 1.05"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="1"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>

    </link>

    <link name = "link_3">
        <visual>
            <geometry>
                <box size="0.4 0.1 0.2"/>
            </geometry>            
            <material name="ABB_Orange"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.4 0.1 0.2"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="1"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>


    </link>

    <link name = "link_4">
        <visual>
            <geometry>
                <cylinder length = "1.725" radius = "0.1"/>
            </geometry>            
            <material name="ABB_Orange"/>
        </visual>
        <collision>
            <geometry>
                <cylinder length = "1.725" radius = "0.1"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="1"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>

    </link>

    <link name = "link_5">
        <visual>
            <geometry>
                <cylinder length = "0" radius = "0.1"/>
            </geometry>            
            <material name="ABB_Orange"/>
        </visual>
        <collision>
            <geometry>
                <cylinder length = "0" radius = "0.1"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="1"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>


    </link>


    <link name = "link_6">
        <visual>
            <geometry>
                <cylinder length = "0" radius = "0.1"/>
            </geometry>            
            <material name="ABB_Orange"/>
        </visual>
        <collision>
            <geometry>
                <cylinder length = "0" radius = "0.1"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="1"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>

    </link>

    <link name ="end_effector">
        <visual>
            <geometry>
                <sphere radius = "0.05"/>
            </geometry>
            <material name="black">
                <color rgba ="0 0 0 1"/> 
            </material>
        </visual>

        <collision>
            <geometry>
                <sphere radius = "0.05"/>
            </geometry>
        </collision>

        <inertial>
            <mass value="0.1"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    
    </link>
    

    <!--Joint Descriptions-->
    <joint name = "joint_1" type ="revolute">        
        <parent link = "base"/>
        <child link = "link_1"/>
        <axis xyz="0 0 1"/>
        <dynamics damping="25.0" friction="25.0"/>
        <limit effort="10.0" lower="-3.14" upper = "3.14" velocity="0.01"/>
        <origin rpy="0 0 0" xyz = "0.24 0 0.4"/>
    </joint>

    <joint name = "joint_2" type ="revolute">        
        <parent link = "link_1"/>
        <child link = "link_2"/>
        <axis xyz="0 1 0"/>
        <dynamics damping="25.0" friction="25.0"/>
        <limit effort="10.0" lower="-1.2" upper = "1.4" velocity="0.01"/>
        <origin rpy="0 0 0" xyz = "0 0 0.525"/>
    </joint>

    <joint name = "joint_3" type ="revolute">        
        <parent link = "link_2"/>
        <child link = "link_3"/>
        <axis xyz="0 1 0"/>
        <dynamics damping="25.0" friction="25.0"/>
        <limit effort="10.0" lower="-0.48" upper = "1.91" velocity="0.01"/>
        <origin rpy="0 0 0" xyz = "-0.2 0 0.625"/>
    </joint>

    <joint name = "joint_4" type ="revolute">        
        <parent link = "link_3"/>
        <child link = "link_4"/>
        <axis xyz="0 0 1"/>
       
        <limit effort="10.0" lower="-5.2" upper = "5.2" velocity="0.01"/>
        <origin rpy="0 1.57 0" xyz = "1.0625 0 0"/>
    </joint>

    <joint name = "joint_5" type ="revolute">        
        <parent link = "link_4"/>
