    </joint>

    <joint name = "joint_6" type ="revolute">        
        <parent link = "link_5"/>
        <child link = "link_6"/>
        <axis xyz="0 0 1"/>
        
        <limit effort="10.0" lower="-5.2" upper = "5.2" velocity="0.01"/>
        <origin rpy="1.57 0 0" xyz = "0 0 0"/>
    </joint>

    <joint name = "joint_7" type ="fixed">        
        <parent link = "link_6"/>
        <child link = "end_effector"/>
        <origin rpy="0 0 0" xyz = "0 0 -0.1"/>
    </joint>


</robot>